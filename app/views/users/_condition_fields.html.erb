<div class="field">
    <%= f.attribute_fields do |a| %>
        <%= a.attribute_select associations: [:role]%>
    <% end %>
    <%= f.predicate_select :only => [:cont] %>
    <%= f.value_fields do |v| %>
        <%= v.text_field :value %>
    <% end %>
    <%= link_to "remove", '#', class:"remove_fields" %>
</div>
