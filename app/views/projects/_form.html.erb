    <%= simple_form_for(@project) do |f| %>
        <%= f.input :name %>
        <br>
        <%= f.input :description %>
        <br>
        <%= f.label :Start_Date %>:
        <br>
        <%= f.date_select :start_date, { :include_blank => true } %>
        <br><br>
        <%= f.input :volunteer_capacity %>
        <br>
        <%= f.input :location %>
        <br>
        <%= f.label :Time_Commitment %>
        <br>
        <%= f.input :hours_per_week %>
        <br>
        <%= f.input :avatar, as: :file, label: "Project Image" %>
        <br>
        <%= f.button :submit, class: "btn btn-md sqs-block-btn btn-black" %><br><br>
    <% end %>
