<div class="row section" style="padding-top: 10px">
  <div class="col-md-11 section">
    <div class="row">
    <div class="col-md-11 col-md-offset-1 text-center">
      <div id="myCarousel" class="carousel slide" data-ride="carousel" >
        <!-- Indicators -->
        <ol class="carousel-indicators text-center">
          <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
          <li data-target="#myCarousel" data-slide-to="1"></li>
          <li data-target="#myCarousel" data-slide-to="2"></li>
        </ol>
    
        <!-- Wrapper for slides -->
        <div class="carousel-inner">
          
          
          <div class="item active">
            <img src=<%= @display_projects[0].avatar_image %> style="width:100%; height:500px">
            <div class="carousel-caption">
              <a href="#" class="text-white" style="color: white"><h3><%= @display_projects[0].name ? @display_projects[0].name : "Project 1" %></h3></a>
              <p class="text-white" style="color: white"></p>
            </div>
          </div>
    
          <div class="item">
            <img src=<%= @display_projects[1].avatar_image %> style="width:100%; height:500px">
            <div class="carousel-caption">
              <a href="#" class="text-white" style="color: white><h3><%= @display_projects[1].name ? @display_projects[1].name : "Project 2" %></h3></a>
              <p class="text-white" style="color: white ></p>
            </div>
          </div>
        
          <div class="item">
            <img src=<%= @display_projects[2].avatar_image %> style="width:100%; height:500px">
            <div class="carousel-caption">
              <a href="#" class="text-white" style="color: white"><h3><%= @display_projects[2].name ? @display_projects[2].name : "Project 3" %></h3></a>
              <p class="text-white" style="color: white"></p>
            </div>
          </div>
        </div>
    
        <!-- Left and right controls -->
        <a class="left carousel-control" href="#myCarousel" data-slide="prev">
          <span class="glyphicon glyphicon-chevron-left"></span>
          <span class="sr-only">Previous</span>
        </a>
        <a class="right carousel-control" href="#myCarousel" data-slide="next">
          <span class="glyphicon glyphicon-chevron-right"></span>
          <span class="sr-only">Next</span>
        </a>
      </div>
      <br>
    </div>
    <div class="col-md-9 col-md-offset-2">
    <h2 class="text-center">Project Listings</h2>
    <hr>
    </div>
    <div class="col-md-9 col-md-offset-2">
      <div class="panel-group" id="accordion">
          <div class="panel panel-default">
             <div class="panel-heading">
                <h4 class="panel-title">
                  <% if @in_demand && @in_demand.count != 0 %>
                   <a data-toggle="collapse" data-parent="#accordion" href="#collapseOne" style="padding-left: 50px">
                     In-Demand Qualities
                   </a>
                </h4>
             </div><!--/.panel-heading -->
             <div id="collapseOne" class="panel-collapse">
                <div class="panel-body">
                   <div class="panel-body">
                      <% @in_demand.each do |quality|  %>
                        <a href = "<%= project_path(quality.project_id) %>" > â€¢ <%= (quality.get_instance).name %> </a> 
                        <% end %>
                     </table>
                  <% end %>
                </div><!--/.panel-body -->
             </div><!--/.panel-collapse -->
          </div><!-- /.panel -->
        </div>
    </div>
    </div>
  </div>
    <div class="col-md-9 col-md-offset-2">
    
    <ul>
    <% @projects.each do |project| %>
    <li>
      <h4><a href="<%= project_path(project.id) %>" ><%= project.name %></a></h4>
      <p><%= project.description %><p>
      </p>
      </p>
     </li>
    <% end %>
  </div>
  </div>
</div>
