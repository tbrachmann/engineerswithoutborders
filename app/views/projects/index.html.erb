
    
<div class="row section">
  <div class="col-md-8 col-md-offset-2 section">
    <h2 class="text-center">Listing Projects</h2>

    <hr>
    <ul>
    <% @projects.each do |project| %>
    <li>
      <h4><a href="<%= project_path(project.id) %>" ><%= project.name %></a></h4>
      <p><%= project.description %><p>
      </p>
      </p>
     </li>
    <% end %>

    <%= paginate @projects %>
  </div>
</div>
